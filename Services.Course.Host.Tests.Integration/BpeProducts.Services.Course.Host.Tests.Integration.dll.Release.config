<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <connectionStrings>
    <add name="DefaultConnection" connectionString="%ConnStrCourseIntegrationTesting%" xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
  </connectionStrings>

  <appSettings>
    <add key="RedisSentinels" value="%RedisSentinels%" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
    <add key="IdentityIssuerThumbprint" value="%IdentityIssuerCertThumbprint%" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
    <add key="UpdateSchema" value="$OctopusVariable.ApplicationUpdateSchema"  xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
  </appSettings>
</configuration>



